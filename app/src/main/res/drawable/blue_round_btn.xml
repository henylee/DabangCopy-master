<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape>
            <corners
                android:radius="10dp" />
            <solid
                android:color="#186DE1" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape>
            <corners
                android:radius="10dp" />
            <solid
                android:color="#085DD1" />
        </shape>
    </item>

</selector>